.header_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.header_swiper swiper image {
  width: 100%;
}
.header_cate {
  display: flex;
}
.header_cate navigator {
  flex: 1;
}
.header_cate navigator image {
  width: 100%;
}
.header-floor .floor_group {
  padding: 10rpx 0;
}
.header-floor .floor_group image {
  width: 100%;
}
.header-floor .floor_list {
  overflow: hidden;
}
.header-floor .floor_list navigator {
  float: left;
  width: 33.33%;
}
.header-floor .floor_list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.header-floor .floor_list navigator:nth-child(2),
.header-floor .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.header-floor .floor_list navigator image {
  width: 100%;
  height: 100%;
}
